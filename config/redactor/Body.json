{
    "buttons": [
        "formatting",
        "bold", 
        "italic", 
        "link", 
        "unorderedlist", 
        "orderedlist",
        "html"
    ],
    "formattingAdd": {
        "h1-add": {
            "title": "H1",
            "api": "module.block.format",
            "args": {
                "tag": "h1",
                "class": "_is-h1",
                "type": "toggle"
            }
        },
        "h2-add": {
            "title": "H2",
            "api": "module.block.format",
            "args": {
                "tag": "h2",
                "class": "_is-h2",
                "type": "toggle"
            }
        }
    },
    "formatting": ["p"],
    "plugins": ["fullscreen", "counter"],
    "toolbarFixed": true,
    "pastePlainText": true,
    "pasteLinkTarget": "_blank",
    "pasteBlockTags": [],
    "pasteInlineTags": [],
    "autoparse": false
}