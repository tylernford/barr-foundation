{
  "require": {
    "craftcms/ckeditor": "^3.4",
    "craftcms/cms": "^4.4.0",
    "craftcms/mailgun": "^3.0",
    "nystudio107/craft-retour": "^4.1",
    "nystudio107/craft-seomatic": "^4.0",
    "nystudio107/craft-twigpack": "^4.0.0-beta.4",
    "putyourlightson/craft-sprig": "^2.6",
    "spacecatninja/imager-x": "^4.1",
    "spicyweb/craft-neo": "^3.8",
    "topshelfcraft/wordsmith": "^4.2",
    "verbb/field-manager": "^3.0",
    "verbb/formie": "^2.0",
    "verbb/hyper": "1.1.8",
    "verbb/super-table": "^3.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
