{
  "require": {
    "cloudgrayau/cleantalk": "^1.0.2",
    "craftcms/ckeditor": "3.6.0",
    "craftcms/cms": "4.6.0",
    "craftcms/feed-me": "^5.3",
    "craftcms/mailgun": "3.0.0",
    "craftcms/redactor": "3.0.4",
    "nystudio107/craft-retour": "^4.1",
    "nystudio107/craft-seomatic": "4.0.37",
    "nystudio107/craft-twigpack": "^4.0.0-beta.4",
    "putyourlightson/craft-sprig": "2.7.3",
    "sebastianlenz/linkfield": "^2.1",
    "spacecatninja/imager-x": "4.2.4",
    "spicyweb/craft-neo": "4.0.2",
    "springworks/entity-diagrams": "^2.0",
    "topshelfcraft/wordsmith": "4.4.0",
    "verbb/cp-nav": "4.0.11",
    "verbb/field-manager": "3.0.8",
    "verbb/formie": "2.1.1",
    "verbb/hyper": "1.1.19",
    "verbb/super-table": "3.0.12",
    "vlucas/phpdotenv": "^5.4.0",
    "wrav/oembed": "^3.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
